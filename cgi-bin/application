use strict;
use warnings;
use ClearPress::controller;

our $VERSION = do { my @r = (q$LastChangedRevision: 67 $ =~ /\d+/mxg); sprintf '%d.'.'%03d' x $#r, @r };

main();
0;

sub main {
  my $util = ClearPress::util->new();
  ClearPress::controller->handler($util);
}
